{
  "main": "./.output/worker/index.js",
  "name": "steakflow",
  "compatibility_date": "2024-05-01",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "assets": {
    "directory": "./.output/static",
    "binding": "ASSETS"
  },
  "build": {
    "command": "npm run build",
    "cwd": "."
  },
  "site": {
    "bucket": "./.output/static"
  },
  "env": {
    "NODE_ENV": "production"
  },
  "triggers": {
    "crons": []
  }
}
